<h1>User List</h1>
<table>
    <thead>
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Actions</th>
    </tr>
    </thead>
<tbody>
    <% users.forEach(user => { %>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td>
                <a href="/user/update?id=<%= user._id %>">Edit User</a>
            </td>
        </tr>
    <% }) %>
</tbody>
</table>
<a href="/home/index">Back to Home</a>